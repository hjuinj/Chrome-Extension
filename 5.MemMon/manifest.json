{
    "manifest_version": 2,
    "name": "Memory Monitor",
    "version": "1.0",
    "description": "Monitors Memory Usage",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "permissions": [
        "system.memory"
    ]
}