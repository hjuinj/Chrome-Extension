{
    "manifest_version": 2,
    "name": "Timer",
    "version": "1.0",
    "description": "Time recording up to 15 min",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts":[
      {
          "matches": ["*://www.zhihu.com/*"],
          "js" : ["content_scripts.js"]
      }  
    ],
    "background": {
        "scripts": [
            "background.js"
            ]
    }
    
}